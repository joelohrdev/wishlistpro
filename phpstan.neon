includes:
    - vendor/larastan/larastan/extension.neon
    - vendor/nesbot/carbon/extension.neon
    - phar://phpstan.phar/conf/bleedingEdge.neon

parameters:

    paths:
        - app
        - bootstrap/app.php
        - config
        - database
        - public
        - routes

    excludePaths:
        - app/Livewire/Settings/TwoFactor.php
        - app/Livewire/Settings/TwoFactor/*

    level: max

    tmpDir: /tmp/phpstan
